{
    "manifest_version": 2,
    "name": "HN Reading Time",
    "version": "1.0",
    "description": "Add reading time to HN articles",
    "author": "Ben Evans",
    "homepage_url": "https://github.com/bencevans/hn-reading-time",
    "content_scripts": 
    [
        {
            "matches": ["*://news.ycombinator.com/*"],
            "js": ["node_modules/jquery/dist/jquery.min.js", "hn-reading-time.user.js"],
            "run_at": "document_end"
        }
    ]
}
